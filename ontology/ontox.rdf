    <!-- http://xmlns.com/foaf/0.1/Image -->

    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Image">
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
