        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontox#Entity"/>
        <rdfs:range rdf:resource="https://w3id.org/ontox#Entity"/>
        <rdfs:comment>Equivalent to dul:associatedWith.</rdfs:comment>
