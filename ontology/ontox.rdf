        <rdfs:range rdf:resource="https://purl.archive.org/o-dang#Manifestation"/>
        <rdfs:comment>A Symbol is manifested in a odang:Manifestation, in the context of a specific Situation and via a certain Modality</rdfs:comment>
        <rdfs:label>manifested in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontox#manifests -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ontox#manifests">
        <rdfs:label>manifests</rdfs:label>
